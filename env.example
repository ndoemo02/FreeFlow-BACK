# FreeFlow Backend Environment Variables
# Copy this file to .env and fill in your actual values

# API Keys
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Feature Flags
FEATURE_UI=on

# Development Settings
NODE_ENV=development
PORT=3002

# API Configuration
API_BASE=http://localhost:3002

# Email Configuration (for n8n workflows)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=alerts@freeflow.com
SMTP_PASS=your_app_password_here

# GitHub Configuration (for n8n workflows)
GITHUB_TOKEN=your_github_token_here
GITHUB_REPO=freeflow/backend
GITHUB_WEBHOOK_URL=your_webhook_url_here
